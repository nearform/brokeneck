{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:import/recommended",
    "plugin:prettier/recommended",
    "plugin:jsx-a11y/recommended",
    "plugin:react-hooks/recommended"
  ],
  "plugins": ["import"],
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "no-console": "error",
    "import/order": [
      "error",
      {
        "newlines-between": "always"
      }
    ]
  }
}
