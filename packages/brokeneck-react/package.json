{
  "name": "brokeneck-react",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Simone Busoli",
    "email": "simone.busoli@nearform.com"
  },
  "main": "cjs/index.js",
  "module": "es/index.js",
  "files": [
    "cjs/",
    "es/",
    "src/"
  ],
  "scripts": {
    "dev": "yarn build:watch",
    "_build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir cjs --copy-files",
    "_build:es": "babel src --out-dir es --copy-files",
    "_build_watch:commonjs": "yarn _build:commonjs --watch",
    "_build_watch:es": "yarn _build:es --watch",
    "clean": "rimraf cjs es",
    "prepare": "yarn build",
    "prebuild": "yarn clean",
    "build": "run-p _build:*",
    "build:watch": "run-p _build_watch:*",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "graphql-hooks": "^5.0.0",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.11.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.5",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "lint-staged": "^10.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "@material-ui/core": "^4.11.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0"
  },
  "lint-staged": {
    "*.{js,md}": [
      "eslint --fix"
    ]
  }
}
