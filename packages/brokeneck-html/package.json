{
  "name": "@nearform/brokeneck-html",
  "version": "1.0.0-spinal.8",
  "author": {
    "name": "Simone Busoli",
    "email": "simone.busoli@nearform.com"
  },
  "repository": "nearform/brokeneck",
  "files": [
    "build"
  ],
  "scripts": {
    "create:env": "node -r fs -e \"fs.copyFileSync('.env.sample', '.env', fs.constants.COPYFILE_EXCL)\"",
    "dev": "yarn start",
    "start": "cross-env PORT=3001 react-scripts start",
    "clean": "rimraf build",
    "prepare": "yarn build",
    "prebuild": "yarn clean",
    "build": "cross-env INLINE_RUNTIME_CHUNK=false react-scripts build",
    "test": "cross-env CI=true react-scripts test",
    "test:dev": "react-scripts test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postbuild": "node scripts/postbuild.js",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@nearform/brokeneck-react": "^1.0.0-spinal.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^12.0.0",
    "cheerio": "^1.0.0-rc.3",
    "cross-env": "^7.0.2",
    "eslint-config-react-app": "^7.0.0",
    "lint-staged": "^12.1.1",
    "rimraf": "^3.0.2"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
