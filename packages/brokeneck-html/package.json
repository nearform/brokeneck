{
  "name": "@nearform/brokeneck-html",
  "version": "1.0.0-spinal.2",
  "author": {
    "name": "Simone Busoli",
    "email": "simone.busoli@nearform.com"
  },
  "repository": "nearform/brokeneck",
  "files": [
    "build"
  ],
  "scripts": {
    "dev": "yarn start",
    "start": "cross-env PORT=3001 react-scripts start",
    "clean": "rimraf build",
    "prepare": "node -e \"fs.existsSync('.env') ? '' : fs.writeFileSync('.env', fs.readFileSync('.env.sample'))\" && yarn build",
    "prebuild": "yarn clean",
    "build": "cross-env INLINE_RUNTIME_CHUNK=false react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postbuild": "node scripts/postbuild.js",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.1",
    "@nearform/brokeneck-react": "^1.0.0-spinal.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.2.2",
    "cheerio": "^1.0.0-rc.3",
    "cross-env": "^7.0.2",
    "lint-staged": "^10.5.2"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
